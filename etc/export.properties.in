# Example file for Libris export using OAI-PMH

name=Exempel
longname=Exempelbiblioteket

# selection
authtype=interleaved
holdtype=interleaved
locations=Te

# settings for bibliographic records
f003=SE-LIBR
lcsh=on
isbnhyphenate=off
issnhyphenate=off
issndehyphenate=off
isbndehyphenate=on
biboperators=Te
move0359=on
sab=on
